package com.rays.preparedstatement;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;

public class Xyz {
	
	public static void main(String[] args) throws Exception {
		testUpdate(int i , String name);
		}

	private static void testUpdate(int i, string name) throws Exception {
		Class.forName("com.mysql.cj.jdbc.Driver");
		Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/advance","root","root");
		PreparedStatement ps= conn.prepareStatement("update marksheet  set name = ? where physics = ?");
	
	
		int i = ps.executeUpdate();			// something ...like putting calues ib menthopd... complete ior
		
		System.out.println(i);
		
		ps.close();
		conn.close();
		

}
